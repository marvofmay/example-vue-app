orm',\n  components: {\n    Input, TextArea, ButtonSendMessage\n  },\n  data () {\n    return {\n      items: [\n        {\n          inputLabelName: 'username',\n          inputType: 'text',\n          inputName: 'username',\n          inputValue: '',\n          inputPlaceholder: 'please, enter your name'\n        },\n        {\n          inputLabelName: 'email',\n          inputType: 'email',\n          inputName: 'email',\n          inputValue: '',\n          inputPlaceholder: 'please, enter your email'\n        }\n      ],\n      textAreaValue: '',\n      errors: []\n    }\n  },\n  props: {\n    msg: String,\n    method: String\n  },\n  methods: {\n    validateFormMessage () {\n      this.errors = []\n      if (!this.items[0].inputValue) {\n        this.errors.push('Username required.')\n      }\n      if (!this.email) {\n        this.errors.push('Email required.')\n      } else if (!this.validEmail(this.items[1].inputValue)) {\n        this.errors.push('Valid email required.')\n      }\n\n      if (!this.errors.length) {\n        return true\n      }\n\n      return false \n    },\n    validEmail (email) {\n      var re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/\n\n      return re.test(email)\n    },\n    handleSubmitMessage () {\n      if (this.validateFormMessage()) {\n          console.log(this.items[0].inputValue, ' ', this.items[1].inputValue, ' ', this.textAreaValue)\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}